{
  "name": "blog-ide",
  "private": true,
  "version": "0.0.0",
  "main": "main.js",
  "author": "senlin",
  "scripts": {
    "_dev": "vite --port 5500",
    "build": "vite build",
    "preview": "vite preview",
    "electron": "wait-on tcp:5500 && cross-env NODE_ENV=development electron .",
    "dev": "concurrently -k \"npm run _dev\" \"npm run electron\"",
