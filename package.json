    "pack": "electron-builder --dir"
  },
  "dependencies": {
    "@unocss/reset": "^0.45.29",
    "antd": "^4.23.5",
    "iconv-lite": "^0.6.3",
    "mobx": "^6.6.2",
    "mobx-react-lite": "^3.4.0",
    "prettier": "^2.7.1",
    "prismjs": "^1.29.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.4.2",
    "react-router-dom": "^6.4.2",
    "vite-plugin-prismjs": "0.0.8"
  },
  "devDependencies": {
    "electron-builder": "^23.6.0",
    "electron": "^21.1.1",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^2.1.0",
    "concurrently": "^7.4.0",
    "cross-env": "^7.0.3",
    "electron-is-dev": "^2.0.0",
    "unocss": "^0.45.29",
    "vite": "^3.1.0",
    "wait-on": "^6.0.1"
  },
